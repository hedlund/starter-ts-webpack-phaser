{
  "name": "starter-ts-webpack-phaser",
  "version": "1.0.0",
  "description": "Yet another webpack, typescript & phaser starter...",
  "main": "src/Game.ts",
  "scripts": {
    "start": "npm run server",
    "server": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.server.js",
    "server:reload": "npm run server -- --inline --hot",
    "build": "npm run build:prod",
    "build:prod": "cross-env NODE_ENV=production webpack --config webpack.config.build.js",
    "build:dev": "cross-env NODE_ENV=development webpack --config webpack.config.build.js",
    "test": "cross-env NODE_ENV=test mocha --compilers js:babel-register --compilers ts:typescript --require ignore-styles --require test.config.js \"src/**/*.tests.ts\"",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "cross-env NODE_ENV=test istanbul cover _mocha -- --compilers ts:typescript --require ignore-styles --require test.config.js \"src/**/*.tests.ts\"",
    "lint": "tslint src/**/*.ts || true"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hedlund/starter-ts-webpack-phaser.git"
  },
  "author": "Henrik Hedlund <henrik@hedlund.im>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hedlund/starter-ts-webpack-phaser/issues"
  },
  "homepage": "https://github.com/hedlund/starter-ts-webpack-phaser#readme",
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.7.7",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-root-import": "^3.2.2",
    "chai": "^3.5.0",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "expose-loader": "^0.7.1",
    "ignore-styles": "^1.2.0",
    "istanbul": "^1.0.0-alpha.2",
    "jsdom": "^8.4.0",
    "json-loader": "^0.5.4",
    "mocha": "^2.4.5",
    "p2": "^0.7.1",
    "phaser": "^2.4.7",
    "phaser-debug": "^1.1.8",
    "phaser-glsl-loader": "0.0.5",
    "postcss-import": "^8.1.0",
    "postcss-loader": "^0.8.2",
    "precss": "^1.4.0",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.1",
    "ts-loader": "^0.8.2",
    "tslint": "^3.8.1",
    "tslint-loader": "^2.1.4",
    "typescript": "^1.8.10",
    "typings": "^0.8.1",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.12.0"
  }
}
